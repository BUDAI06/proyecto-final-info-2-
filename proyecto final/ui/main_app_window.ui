  <property name="styleSheet">
   <string>/* Estilo Global UdeA - Ingeniería */
QWidget {
    background-color: #F4F6F8; /* Fondo general gris muy claro para descanso visual */
    color: #1A1A1A; /* Texto casi negro para mejor lectura técnica */
    font-family: &quot;Inter&quot;, &quot;Roboto&quot;, &quot;Segoe UI&quot;, sans-serif;
    font-size: 13px;
}

/* Área Central */
QMainWindow, QWidget#centralwidget {
    background-color: #FFFFFF;
}

/* Agrupadores y Contenedores */
QGroupBox {
    background-color: #FFFFFF;
    border: 1px solid #D1D5DB;
    border-radius: 4px;
    margin-top: 12px;
    padding: 8px;
    font-weight: 600;
}
QGroupBox::title {
    subcontrol-origin: margin;
    subcontrol-position: top left;
    left: 10px;
    padding: 0 4px;
    color: #005931; /* Verde UdeA */
    font-size: 14px;
}

/* Etiquetas */
QLabel#titleLabel, QLabel[objectName~=&quot;title&quot;] {
    font-size: 18px;
    font-weight: 700;
    color: #005931; /* Títulos en verde institucional */
}

/* Botones Principales */
QPushButton {
    background-color: #005931; /* Verde Institucional UdeA */
    color: white;
    border: 1px solid #004526;
    border-radius: 4px;
    padding: 6px 16px;
    min-height: 30px;
    font-weight: 600;
}
QPushButton:hover {
    background-color: #2D7A4D; /* Verde un poco más claro para hover */
    border: 1px solid #005931;
}
QPushButton:pressed {
    background-color: #003D22; /* Verde muy oscuro al presionar */
}
QPushButton:disabled {
    background-color: #E0E0E0;
    color: #9E9E9E;
    border: 1px solid #BDBDBD;
}

/* Botones Secundarios o Planos */
QPushButton[flat=&quot;true&quot;] {
    background: transparent;
    color: #005931;
    border: none;
}
QPushButton[flat=&quot;true&quot;]:hover {
    text-decoration: underline;
}

/* Entradas de Texto (Inputs) */
QLineEdit, QComboBox, QSpinBox, QDoubleSpinBox {
    background: #FFFFFF;
    border: 1px solid #CED4DA;
    border-radius: 4px;
    padding: 5px;
    selection-background-color: #2D7A4D; /* Selección verde */
    selection-color: white;
}
QLineEdit:focus, QComboBox:focus {
    border: 2px solid #005931; /* Foco en verde UdeA */
}

/* Tablas de Datos */
QTableWidget, QTableView {
    background-color: #FFFFFF;
    gridline-color: #E0E0E0;
    selection-background-color: #E8F5E9; /* Fondo verde muy pálido para fila seleccionada */
    selection-color: #000000; /* Texto negro en selección para contraste */
    border: 1px solid #D1D5DB;
}
QHeaderView::section {
    background-color: #005931; /* Cabecera de tabla en verde */
    color: white;
    padding: 6px;
    border: none;
    font-weight: bold;
}

/* Sliders */
QSlider::groove:horizontal {
    border: 1px solid #bbb;
    background: white;
    height: 8px;
    border-radius: 4px;
}
QSlider::sub-page:horizontal {
    background: #005931; /* Relleno del slider en verde */
    border-radius: 4px;
}
QSlider::handle:horizontal {
    background: #FFFFFF;
    border: 1px solid #005931;
    width: 16px;
    height: 16px;
    margin: -5px 0;
    border-radius: 8px;
}

/* Scrollbars Minimalistas */
QScrollBar:vertical {
    width: 10px;
    background: transparent;
}
QScrollBar::handle:vertical {
    background: #CFD8DC;
    border-radius: 5px;
}
QScrollBar::handle:vertical:hover {
    background: #B0BEC5;
}

/* Mensajes de Estado */
QLabel[style~=&quot;status-error&quot;] { color: #D32F2F; font-weight: bold; } /* Rojo estándar para errores */
QLabel[style~=&quot;status-warning&quot;] { color: #F57F17; font-weight: bold; }
</string>
